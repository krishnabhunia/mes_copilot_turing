{
    "loglevel": "DEBUG",
    "badrequest": 400,
    "statusok": 200,
    "CORS_HEADERS": "Content-Type",
    "host": "logging_fastapi_server",
    "port": 7404,
    "logfileName": "logfile.log",
    "base_logdirectory" : "/app/log_directory",
    "logURL": "http://logging_fastapi_server:7404/uploadToLogfile",
    "logHandler": {
        "mode": "a",
        "maxBytes": 3145728,  
        "backupCount": 10,  
        "delay": false     
    },
        "cors": {
          "allow_origins": ["*"],
          "allow_credentials": true,
          "allow_methods": ["GET", "PUT", "POST", "DELETE", "OPTIONS"],
          "allow_headers": ["Content-Type", "Authorization"],
          "max_age": 86400
        }
      
}



